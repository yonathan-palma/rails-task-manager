<h1>New Task</h1>
<%= form_with model: @task, class: "doggos_form" do |f| %>
  <%= f.label :title, class: "form-label" %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.submit %>
<% end %>
<%= link_to "Back to list", tasks_path, class: "text-decoration-none" %>
